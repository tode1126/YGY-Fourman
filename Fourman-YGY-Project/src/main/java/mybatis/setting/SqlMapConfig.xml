<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org// DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="spring.data.BoardDto" alias="bdto"/>
		<typeAlias type="spring.data.noticeDto" alias="ndto"/>
		<typeAlias type="spring.data.UserDto" alias="userDto"/>
	</typeAliases>
	<mappers>
		<mapper resource="mybatis/sql/BoardSql.xml"/>
		<mapper resource="mybatis/sql/noticesql.xml"/>
		<mapper resource="mybatis/sql/UserSql.xml"/>
		<mapper resource="mybatis/sql/AdminSql.xml"/>
	</mappers>
</configuration>